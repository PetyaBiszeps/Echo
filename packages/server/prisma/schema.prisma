generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model User {
  id String @id @default(cuid())
  username String @unique @db.VarChar(50)
  password String
  createdAt DateTime @default(now()) @db.Timestamptz

  @@index([username])
  @@map("users")
}